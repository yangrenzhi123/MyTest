<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level %logger{36} %M %L - %msg%xEx%n" />
		</Console><!-- 
		<File name="log" fileName="/log/service.check.log" append="false">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level %logger{36} %M %L - %msg%xEx - 11 - %n" />
		</File> -->
	</Appenders>
	<Loggers>
		<Root level="INFO">
			<AppenderRef ref="Console" /><!-- 
			<AppenderRef ref="log" /> -->
		</Root>
	</Loggers>
</Configuration>