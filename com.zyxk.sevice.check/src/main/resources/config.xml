<?xml version="1.0" encoding="UTF-8"?>
<!-- 
start 1表示启动监控
frequency 300000 表示300秒执行一次检查
process port 表示服务端口 exec表用其中的命令启动程序 execTime经过多少时间，执行下一个程序


nohup java -DconfigFilePath=/opt/check/config.xml -Djava.ext.dirs=/opt/check/lib -cp /opt/check/com.zyxk.sevice.check-0.0.1-SNAPSHOT.jar com.zyxk.sevice.check.BootController > nohup.check.out & cd .
 -->
<config>
	<frequency>30000</frequency>
	<start>1</start>
	<processes>
		<process>
		        <port>80</port>
		        <exec>/usr/local/nginx/sbin/nginx</exec>
		        <execTime>10000</execTime>
		</process>
		<process>
		        <port>2181</port>
		        <exec>docker start zookeeper</exec>
		        <execTime>10000</execTime>
		</process>
		<process>
		        <port>8066</port>
		        <exec>/opt/mycat/bin/mycat start</exec>
		        <execTime>10000</execTime>
		</process>
		<process>
		        <port>8761</port>
		        <exec>java -Djava.ext.dirs=/data/applictaion/eurekaService/lib -cp /data/applictaion/eurekaService/eureka.jar com.example.demo.EurekaServerApplication</exec>
		        <execTime>10000</execTime>
		</process>
		<process>
		        <port>8886</port>
		        <exec>java -jar /data/applictaion/zuulService/zuulServer.jar</exec>
