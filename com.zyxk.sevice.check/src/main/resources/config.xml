		        <port>27003</port>
		        <exec>mongod --bind_ip 0.0.0.0 --port 27003 --dbpath=/mgdata/cnf  --replSet rc/192.168.30.120:27003 --journal --configsvr</exec>
		        <execTime>10000</execTime>
		</process>
		<process>
		        <port>27017</port>
		        <exec>mongos --bind_ip 0.0.0.0 --port 27017 --configdb rc/192.168.10.10:27003,192.168.30.120:27003,192.168.30.121:27003</exec>
		        <execTime>10000</execTime>
		</process>
