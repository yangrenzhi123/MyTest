		        <exec>mongod --bind_ip 0.0.0.0 --port 27001 --oplogSize 100 --dbpath /mgdata/shard1 --replSet r1/192.168.30.120:27001 --journal --shardsvr</exec>
		        <execTime>10000</execTime>
		</process>
		<process>
		        <port>27002</port>
		        <exec>mongod --bind_ip 0.0.0.0 --port 27002 --oplogSize 100 --dbpath /mgdata/shard2 --replSet r2/192.168.30.120:27002 --journal --shardsvr</exec>
		        <execTime>10000</execTime>
