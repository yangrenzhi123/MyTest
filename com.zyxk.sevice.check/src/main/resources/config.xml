		</process>
		<process>
		        <port>8889</port>
		        <exec>java -Djava.ext.dirs=/data/applictaion/configService/lib -cp /data/applictaion/configService/config.jar com.yang.test.java.spring.cloud.configserver.ConfigServerApplication --spring.cloud.config.server.native.search-locations=/data/applictaion/configService/config</exec>
